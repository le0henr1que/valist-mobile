{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-primary.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo-primary.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",

      "imageSize": {
        "width": 100,
        "height": 100
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "image": "./assets/logo-primary.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font"]
  }
}
